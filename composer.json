{
    "name": "mrcoto/migration-workflow",
    "description": "Run migration and seed in custom order",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Espinoza Salas",
            "email": "j.espinoza@innlab.cl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0",
        "illuminate/database": "5.6.*|5.7.*|5.8.*|^6.0",
        "monolog/monolog": "^1.23|^2.0",
        "bramus/monolog-colored-line-formatter": "~2.0|~3.0",
        "haydenpierce/class-finder": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "MrCoto\\MigrationWorkflow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrCoto\\MigrationWorkflow\\Test\\": "tests"
        },
        "classmap": [
            "tests/Stub/seeders"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrCoto\\MigrationWorkflow\\Application\\LaravelMigrationWorkflowServiceProvider"
            ]
        }
    }
}
